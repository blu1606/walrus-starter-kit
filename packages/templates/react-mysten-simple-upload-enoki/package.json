{
  "name": "{{projectName}}",
  "version": "0.1.0",
  "private": true,
  "description": "Walrus application with Enoki zkLogin authentication scaffolded with create-walrus-app",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint . --ext .ts,.tsx",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "setup-walrus-deploy": "bash scripts/setup-walrus-deploy.sh",
    "deploy:walrus": "~/.walrus/bin/site-builder --context=testnet deploy ./dist --epochs 10",
    "walrus:portal": "bash scripts/run-portal.sh"
  },
  "dependencies": {
    "@mysten/dapp-kit": "^0.20.0",
    "@mysten/enoki": "^0.15.0",
    "@mysten/sui": "1.45.2",
    "@mysten/walrus": "^0.9.0",
    "@tanstack/react-query": "^5.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
